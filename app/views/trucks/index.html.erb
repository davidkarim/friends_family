<p id="notice"><%= notice %></p>

<meta name="viewport" content="width=device-width, initial scale =1"
<link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">






<div class="container">
  <h2>Hungry?</h2>
  <p>Check out the Food Trucks available:</p>
  <table class="table">
    <thead>
      <tr>
        <th>Name</th>
        <th>Website</th>
        <th>Location</th>
        <th>Phone Number</th>
      </tr>
    </thead>
    <tbody>
      <% @trucks.each do |truck| %>
      <tr>
        <td><%= truck.name %></td>
        <td><%= truck.web_url %></td>
        <td>123 NW 2nd Ave Ft.Lauderdale,FL 33333</td>
        <td>111-111-1111</td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>

</body>
</html>
